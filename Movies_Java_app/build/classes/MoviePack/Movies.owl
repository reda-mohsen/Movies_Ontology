    <owl:Class rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Genre">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/isGenreOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/ProductionCompany"/>
        <rdfs:comment>Categories or styles of movies, such as action, comedy, drama, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie -->

    <owl:Class rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/HasActor"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Actor"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/HasDirector"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Director"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/HasGenre"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Genre"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/HasProducionCompany"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/ProductionCompany"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/HasWriter"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Writer"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/ProductionCompany"/>
