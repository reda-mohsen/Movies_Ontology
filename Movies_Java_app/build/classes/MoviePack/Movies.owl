    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/age -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/age">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Person"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/age"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minExclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>Indicates the age of a person.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/country -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/country">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Indicates the country of a movie.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/gender -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/gender">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Person"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/gender"/>
                        <owl:hasValue>female</owl:hasValue>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/gender"/>
                        <owl:hasValue>male</owl:hasValue>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Indicates the gender of a person.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/genre -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/genre">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Genre"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
