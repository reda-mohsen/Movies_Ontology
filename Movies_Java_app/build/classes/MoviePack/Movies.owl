    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Actor -->

    <owl:Class rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Actor">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/isActorOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Individuals who play a role in a movie.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Director -->

    <owl:Class rdf:about="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Director">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Person"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/isDirectorOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/redar/ontologies/2023/4/Movies/Movie"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>Individuals who direct a movie.</rdfs:comment>
    </owl:Class>
    


